    "core-js": "^3.6.5",
    "jwt-decode": "^3.1.2",
    "quasar": "^2.5.5",
    "vue": "^3.0.0",
    "vue-custom-select": "^1.0.10",
    "vue-i18n": "^9.0.0",
    "vue-router": "^4.0.0",
